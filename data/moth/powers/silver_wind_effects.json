{
    "type":"origins:action_over_time",
    "entity_action":{
       "type":"origins:and",
       "actions":[
          {
             "type":"origins:area_of_effect",
             "radius":3,
             "bientity_action":{
                "type":"origins:damage",
                "amount":8,
                "damage_type":"minecraft:sting"
             },
             "bientity_condition":{
                "type":"origins:target_condition",
                "condition":{
                   "type":"origins:origin",
                   "origin":"moth:moth",
                   "layer":"origins:origin",
                   "inverted":true
                }
             },
             "include_target":false
          },
          {
             "type":"origins:execute_command",
             "command":"tp @s ^ ^ ^0.3"
          },
          {
             "type":"origins:spawn_particles",
             "particle":"minecraft:falling_nectar",
             "count":50,
             "speed":0.1,
             "force":true,
             "spread":{
                "x":1.0,
                "y":1.0,
                "z":1.0
             },
             "offset_y":0.0
          },
          {
             "type":"origins:spawn_particles",
             "particle":"minecraft:falling_nectar",
             "count":20,
             "speed":0.2,
             "force":true,
             "spread":{
                "x":1.0,
                "y":0.5,
                "z":1.0
             },
             "offset_y":0.0
          }
       ]
    },
    "rising_action":{
       "type":"origins:and",
       "actions":[
          {
             "type":"origins:play_sound",
             "sound":"minecraft:item.elytra.flying",
             "volume":1.00,
             "pitch":1.00
          },
          {
             "type":"origins:execute_command",
             "command":"scale set pehkui:height 0.25 @s"
          },
          {
             "type":"origins:execute_command",
             "command":"scale set pehkui:width 0.25 @s"
          },
          {
             "type":"origins:execute_command",
             "command":"scale persist set true @s"
          },
          {
             "type":"origins:delay",
             "action":{
                "type":"origins:execute_command",
                "command":"kill @s"
             },
             "ticks":200
          }
       ]
    },
    "interval":1
}